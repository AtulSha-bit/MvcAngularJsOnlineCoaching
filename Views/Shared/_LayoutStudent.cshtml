        .logo-container {
            display: flex;
            align-items: center;
        }

            .logo-container img {
                width: 6rem;
                height: fit-content;
            }

        .folk {
            margin-left: 10px;
            margin-top: 8px;
        }

            .folk h5 {
                color: #212529;
            }

        .logo {
            font-size: 1.1rem;
            font-weight: 600;
            letter-spacing: 2px;
            margin-bottom: 0px;
            /*text-transform: uppercase;*/
            /* line-height: 3rem;*/
        }
        .input-group-prepend {
            margin-right: -1px;
            height: 27px;
        }
        .fa {
            margin-left: 6px;
        }

        .dropdown {
            opacity: 1;
        }

        .heading_5, h5 {
            font-size: 16px;
            line-height: 1.25;
            font-weight: 600;
        }

        #header .max-width-container .navbar .collapse .navbar-nav li a {
            /* font-family: 'Inter var',sans-serif; */
            padding-left: 7px !important;
            padding-right: 7px !important;
            border-top: 2px solid transparent !important;
            border-bottom: 2px solid transparent !important;
            height: 100% !important;
            display: flex !important;
            align-items: center !important;
            font-size: 15px;
            font-weight: 600;
            color: black;
        }

            #header .max-width-container .navbar .collapse .navbar-nav li a:hover {
                text-decoration: none;
            }

        @@media (min-width: 576px) {
            .modal-dialog {
                max-width: 500px;
                margin: 9rem auto;
            }
        }

        .modal-content {
            background: linear-gradient(75deg, #45b3e0 10%, white) !important;
        }

        .form-control {
            height: 27px;
            font-size: 11px;
        }

        label {
            font-size: 13px;
        }
    </style>
</head>
