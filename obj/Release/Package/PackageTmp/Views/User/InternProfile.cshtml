
@{
    ViewBag.Title = "InternProfile";
    Layout = "~/Views/Shared/LayoutIntern.cshtml";
}

<style>
    #firstrow {
        margin-left: 4rem;
        margin-right: 4rem;
        width:100%;
    }

    /*  .btn {
        border-radius: 0rem;
        color: white !important;
    }
       */

    .card {
        border-radius: 10px;
    }
</style>

<link href="~/Css/internprofile.css" rel="stylesheet" />
@*studentprofileinit();callInit();*@
<div ng-app="app" ng-controller="MyCtrl" ng-init="internprofileinit();documentinit();internshipdetailsinit()">
    <div class="container" ng-hide="hideStudentProflie">

        <div class="row" id="studentdata">
            <div class="col-md-3" id="side" style="width: -webkit-fill-available; height: auto;">
                <div ng-show="ImgOnlyOnEdit" class="imgname">
                    @if (Session["InternImage"] != null)
                    {
                        @*<img ng-src="~/ProjectImages/@Session["InternImage"].ToString()" />*@
                        <img ng-src="~/Intern/Images/@Session["InternImage"].ToString()" />
                    }
                    else
                    {
                        <img src="~/Images/kimberly-farmer-lUaaKCUANVI-unsplash.jpg" />
                    }
                    @if (Session["InternUser"] != null)
                    {
                        <h2>@Session["InternUser"].ToString()</h2>
                    }

                </div>

                <div class="list-group" id="list-tab" role="tablist">
                    <a class="list-group-item list-group-item-action active" id="list-home-list" data-toggle="list" href="#list-Profile" role="tab" aria-controls="Profile"> Profile</a>
                    <a class="list-group-item list-group-item-action" id="list-home-list" data-toggle="list" href="#list-Upload" role="tab" aria-controls="Upload"> Upload Image</a>
                    <a class="list-group-item list-group-item-action" id="list-profile-list" data-toggle="list" href="#list-Basic" role="tab" aria-controls="Basic"> Basic Information</a>
                    @*<a class="list-group-item list-group-item-action" id="list-profile-list" data-toggle="list" href="#list-Qualification" role="tab" aria-controls="Qualification"> Qualification</a>

                    <a class="list-group-item list-group-item-action" id="list-profile-list" data-toggle="list" href="#list-Document" role="tab" aria-controls="Document">Document</a>*@

                    @*   <a class="list-group-item list-group-item-action" id="list-messages-list" data-toggle="list" href="#list-Course" role="tab" aria-controls="messages">Internship Details</a> *@
                    @*   <a class="list-group-item list-group-item-action" id="list-settings-list" data-toggle="list" href="#list-Payment" role="tab" aria-controls="settings"> Payment Mode</a>*@
                    <a class="list-group-item list-group-item-action" id="list-settings-list" data-toggle="list" href="#list-Settings" role="tab" aria-controls="settings">Settings</a>
                    @*<a class="list-group-item list-group-item-action" id="list-settings-list" data-toggle="list" href="#list-Communication" role="tab" aria-controls="settings" ng-click="GetCommunicationById('All-Intern-Students');">Communication</a>*@
                    @*     <a class="list-group-item list-group-item-action" id="list-settings-list" data-toggle="list" href="#list-Close" role="tab" aria-controls="settings">Close account</a>  Intern *@

                </div>
            </div>
            <div class="col-md-9" id="info">

                <div class="tab-content" id="nav-tabContent">
                    <div class="tab-pane fade show active" id="list-Profile" role="tabpanel" aria-labelledby="list-home-list">
                        <div class="top">
                            <h2>Intern Profile</h2>

                            <h3>Information</h3>
                        </div>
                        <form class="inner2" name="studentprofiletab" novalidate>

                            <div class="form-group">
                                <label class="control-label">Name</label>
                                <input type="text" ng-model="Name" class="form-control" name="name" maxlength="45" required ng-pattern="/^[a-z A-Z]+$/"  />

                                <span style="color:Red" ng-show="studentprofiletab.name.$error.required && studentprofiletab.name.$touched" ng-cloak>
                                    Name is required
                                </span>

                                <span style="color:Red" ng-show="studentprofiletab.name.$dirty && studentprofiletab.name.$error.pattern" ng-cloak>
                                    Name must contain alphabets (A-Z and a-z) only
                                </span>
                            </div>

                            <div class="form-group">

                                <div>
                                    <label class="control-label" for="inputdob">
                                        @*  <span style="color:red">* </span>*@
                                        DOB
                                    </label>
                                    <input class="form-control" moment-picker="momentDOB" required format="DD/MM/YYYY" start-view="day" name="dob" ng-model="DOB"
                                           ng-model-options="{ updateOn: 'blur' }" ng-blur="validateInternDOB()" />

                                    <span style="color:Red" ng-show="studentprofiletab.DOB.$error.required && studentprofiletab.DOB.$touched" ng-cloak>
                                        DOB is required
                                    </span>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="control-label">Username</label>
                                <input type="text" ng-model="Username" class="form-control" name="username" disabled required maxlength="45"  />
                                <span style="color:Red" ng-show="studentprofiletab.username.$error.required && studentprofiletab.username.$touched" ng-cloak>
                                    Username is required
                                </span>
                            </div>
                            @*<div class="form-group" style="display:none">
                                <label class="control-label">Password</label>
                                <input type="password" ng-model="Password" class="form-control" name="password" maxlength="40" disabled required />

                                <span style="color:Red" ng-show="studentprofilereg.password.$error.required && studentprofilereg.password.$touched" ng-cloak>
                                    Password is required
                                </span>
                            </div>*@

                            <div class="form-group ">
                                <label class="control-label">Email</label>
                                <input type="text" ng-model="Email" class="form-control" name="email" maxlength="50" required ng-pattern="emailregex"  />

                                <span style="color:Red" ng-show="studentprofiletab.email.$error.required && studentprofiletab.email.$touched" ng-cloak>
                                    Email is required
                                </span>
                                <span style="color:Red" ng-show="studentprofiletab.email.$dirty && studentprofiletab.email.$error.pattern" ng-cloak>
                                    Invalid Email
                                </span>
                            </div>

                            <div class="form-group">
                                <label>Phone No</label>
                                <input type="text" class="form-control" id="exampleInputphon" name="mobile" autocomplete="off" ng-model="Phone" maxlength="10" required ng-pattern="/^[6-9][0-9]{9}$/" />

                                <span style="color:Red" ng-show="studentprofiletab.mobile.$error.required && studentprofiletab.mobile.$touched" ng-cloak>
                                    Mobile No. is required
                                </span>

                                <span style="color:Red" ng-show="studentprofiletab.mobile.$dirty && studentprofiletab.mobile.$error.pattern" ng-cloak>
                                    10 digits only must start with 6,7,8,9
                                </span>
                            </div>
                            <hr />
                            <h5 class="pb-3">Links</h5>
                            <div class="form-group">


                                <div class="input-group mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">http://twitter.com/</div>
                                    </div>
                                    <input type="text" class="form-control" id="inlineFormInputGroup" ng-model="Twitter" placeholder="Username">
                                </div>

                            </div>

                            <div class="form-group">


                                <div class="input-group mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">http://www.facebook.com/</div>
                                    </div>
                                    <input type="text" class="form-control" id="inlineFormInputGroup2" ng-model="Facebook"
                                           placeholder="Username">
                                </div>

                            </div>
                            @*<div class="form-group">


                                <div class="input-group mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">http://www.linkedin.com/</div>
                                    </div>
                                    <input type="text" class="form-control" id="inlineFormInputGroup3" placeholder="Username">
                                </div>

                            </div>*@
                            <div class="form-group">


                                <div class="input-group mb-2">
                                    <div class="input-group-prepend">
                                        <div class="input-group-text">http://www.instagram.com/</div>
                                    </div>
                                    <input type="text" class="form-control" id="inlineFormInputGroup4" ng-model="Instagram" placeholder="Username">
                                </div>

                            </div>

                        </form>
                        <div class="row" id="butk">
                            <button type="submit" class="btn btn-primary" ng-click="UpdateInternProfile()" ng-disabled="studentprofiletab.$invalid">Save</button>
                        </div>
                    </div>

                    <div class="tab-pane fade" id="list-Upload" role="tabpanel" aria-labelledby="list-home-list">
                        <div class="top">
                            <h2>Upload Image</h2>

                            <h3>Information</h3>
                        </div>
                        <form class="inner2">
                            <div class="form-group">
                                <label class="control-label">Image preview</label>
                                <div ng-show="PreviewImage" style="border-radius:50%;text-align: center;">
                                    @* previ  src="~/Images/63081-profile-computer-social-user-icons-free-frame-thumb.png"*@
                                    <img height="200" width="200" class="prevClass pt-3" data-ng-src="{{Preview}}" />
                                </div>
                            </div>

                            @*  validatedCustomFile"*@

                            <div class="form-group pt-3">
                                <label class="control-label">Add / Change Image</label>
                                <div class="custom-file mb-3">
                                    <input type="file" class="custom-file-input" id="validateCustomFile" ngf-select="changeInternImg($file)" ng-model="file" name="file" ngf-pattern="'image/*'"
                                           ngf-accept="'image/*'" ngf-max-size="20MB" ngf-min-height="100" ngf-resize="{width: 200, height: 200}" required>
                                    <label class="custom-file-label" for="validatedCustomFile">Choose file...</label>

                                </div>
                            </div>



                        </form>
                        <div class="row" id="butk" style="margin-top: 48px">
                            <button type="submit" class="btn btn-primary" ng-click="UpdateInternProfile()">Save</button>
                        </div>
                    </div>

                    <div class="tab-pane fade" id="list-Basic" role="tabpanel" aria-labelledby="list-profile-list">
                        <div class="top">
                            <h2> Basic Information</h2>

                            <h3>Information</h3>
                        </div>
                        <form class="inner2" name="profilebasictab" novalidate>

                            <div class="form-group">
                                <label for="inputState">State</label>
                                <input type="text" class="form-control" ng-model="State" name="state" maxlength="30" required  autocomplete="off" />
                                <span style="color:Red" ng-show="profilebasictab.state.$error.required && profilebasictab.state.$touched" ng-cloak>
                                    State is required
                                </span>
                            </div>

                            <div class="form-group">
                                <label for="inputCity"> City</label>
                                <input type="text" class="form-control" ng-model="City"  name="city" id="inputCity" required maxlength="30">
                                <span style="color:Red" ng-show="profilebasictab.city.$error.required && profilebasictab.city.$touched" ng-cloak>
                                    City is required
                                </span>
                            </div>

                            <div class="form-group">
                                <label for="inputZip"> Pin Code</label>
                                <input type="text" class="form-control"  ng-model="PinCode" id="inputZip" name="PinCode" required maxlength="6">
                                <span style="color:Red" ng-show="profilebasictab.PinCode.$error.required && profilebasictab.PinCode.$touched" ng-cloak>
                                    Pin Code is required
                                </span>
                            </div>

                            <div class="form-group">
                                <label for="inputAddress"> Address</label>
                                <textarea rows="3" cols="30" ng-model="Address"  name="address" class="form-control"
                                          maxlength="100" required autocomplete="off">
                                        </textarea>
                                <span style="color:Red" ng-show="profilebasictab.address.$error.required && profilebasictab.address.$touched" ng-cloak>
                                    Address is required
                                </span>
                            </div>

                            <div class="form-group">
                                <label for="inputAddress2">LandMark</label>
                                <textarea class="form-control" rows="3" cols="30" ng-model="LandMark" id="landmark" name="landmark" maxlength="100" autocomplete="off">
                                        </textarea>
                            </div>

                        </form>
                        <div class="row" id="butk">
                            <button type="submit" class="btn btn-primary" ng-click="UpdateInternProfile()" ng-disabled="profilebasictab.$invalid">Save</button>
                        </div>
                    </div>




                    <div class="tab-pane fade" id="list-Qualification" role="tabpanel" aria-labelledby="list-profile-list">
                        <div class="top">
                            <h2>Qualification</h2>

                            <h3>Information</h3>
                        </div>
                        <form class="inner2" name="qualificationreg" novalidate>

                            <div class="form-group">
                                <label for="inputState">Qualification</label>
                                <input type="text" class="form-control" ng-model="Qualification" disabled name="qualification" maxlength="30" required autocomplete="off" />
                                <span style="color:Red" ng-show="qualificationreg.qualification.$error.required && qualificationreg.qualification.$touched" ng-cloak>
                                    Qualification is required
                                </span>
                            </div>

                            <div class="form-group">
                                <label for="inputCity"> College / University</label>
                                <input type="text" class="form-control" ng-model="CollegeUniv" name="collegeuniv" id="inputCollegeUniv" maxlength="40" required>
                                <span style="color:Red" ng-show="qualificationreg.collegeuniv.$error.required && qualificationreg.collegeuniv.$touched" ng-cloak>
                                    College/University is required
                                </span>
                            </div>

                            <div class="form-group">
                                <label for="inputZip"> Year Of Passing</label>
                                <input type="text" class="form-control" ng-model="YearOfPassing" id="yearOfPassing" name="yearofpassing" maxlength="6">

                            </div>

                            <div class="form-group">
                                <label for="inputAddress"> How Do You Know</label>
                                <textarea rows="3" cols="30" ng-model="HowDoYouKnow" disabled name="howdoyouknow" class="form-control"
                                          maxlength="100" autocomplete="off">
                                        </textarea>

                            </div>

                            @*<div class="form-group">
                                <label for="inputAddress2">Comments</label>
                                <textarea class="form-control" rows="3" cols="30" ng-model="Comments"
                                          id="Comments" name="comments" maxlength="100" autocomplete="off">
                                        </textarea>
                            </div>*@

                        </form>
                        <div class="row" id="butk">
                            <button type="submit" class="btn btn-primary" ng-click="UpdateInternProfile()" ng-disabled="qualificationreg.$invalid">Save</button>
                        </div>
                    </div>



                    <div class="tab-pane fade" id="list-Document" role="tabpanel" aria-labelledby="list-profile-list">
                        <div class="top">
                            <h2>Documents</h2>

                            <h3>Information</h3>
                        </div>
                        <form class="inner2" name="documentreg" novalidate>



                            <div class="form-group">
                                <label for="inputState">Aadhar Number</label>
                                <input type="text" class="form-control" ng-model="mdlInternApplyAadhar" name="aadhaar" maxlength="30" required autocomplete="off" />
                                <span style="color:Red" ng-show="documentreg.aadhaar.$error.required && documentreg.aadhaar.$touched" ng-cloak>
                                    Aadhar Number is required
                                </span>
                            </div>




                            <div class="form-group pt-1">
                                <label class="control-label">Upload Aadhar Card Image</label>
                                <div class="custom-file mb-3">
                                    <input type="file" class="custom-file-input" id="validatedCustomFile" ngf-select="uploadAadhaarForInterns($file)" ng-model="fileAadhar" name="file" ngf-pattern="'image/*'"
                                           ngf-accept="'image/*'" ngf-max-size="5MB" ngf-resize="{width: 300, height: 500}" />
                                    <label class="custom-file-label" for="validatedCustomFile">Choose file...</label>
                                    <p><b>Note:</b>Only Image file.</p>

                                    <div ng-hide="hideaadhar"><b> Document Name: </b>  {{ fileAadhar }} </div>

                                    <div ng-hide="previewaadhar"> <b> Document Name: </b> {{ aadhaarFileName }} </div>
                                    @*    {{ aadhaarFileName }}*@
                                </div>
                            </div>


                            <div class="form-group pt-1">
                                <label class="control-label">Upload Resume</label>
                                <div class="custom-file mb-3">
                                    <input type="file" class="custom-file-input" ngf-select="uploadResumeForInterns($file)" ng-model="fileResume" name="file" ngf-pattern="'.pdf'"
                                           ngf-accept="'.pdf'" ngf-max-size="5MB" ngf-resize="{width: 300, height: 500}" />
                                    <label class="custom-file-label" for="validatedCustomFile">Choose file...</label>
                                    <p><b>Note:</b>Only pdf file.</p>

                                    <div ng-hide="hideresume"> <b>Resume:</b>   {{  fileResume }} </div>
                                    <div ng-hide="previewresume"> <b> Resume: </b> {{ resumeFileName }} </div>

                                </div>
                            </div>

                        </form>
                        <div class="row" id="butk">
                            <button type="submit" class="btn btn-primary" ng-click="UpdateInternProfile()" ng-disabled="documentreg.$invalid">Save</button>
                        </div>
                    </div>



                    <div class="tab-pane fade" id="list-Course" role="tabpanel" aria-labelledby="list-messages-list">
